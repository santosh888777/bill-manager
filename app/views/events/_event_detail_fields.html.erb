<fieldset>
  <label class="form-label">Payee Name</label>
  <%= f.collection_select(:user_id, User.all, :id, :name, prompt: "Select Payee")  %><br/>
  <label class="form-label">Paid Amount</label>
  <%= f.text_field :paid_amount, :class=>"form-control", style: "width: 12%;" %><br/>
  <label class="form-label">Is Present?</label>
  <%=f.check_box :is_present, {} , "yes", "no"  %>
  <%= f.hidden_field :_destroy %><br/>
  <%= link_to "Remove", '#', :class => 'remove_fields' %>
</fieldset>

